{
  "name": "ucloud_ui",
  "version": "0.1.0",
  "description": "isomorphic web app for ucloud ui.",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015-node5"
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "NODE_ENV=development node src/server/index.js",
    "install": "",
    "debug:client": "BABEL_ENV=development NODE_ENV=development node src/server/compiler/index.jsx",
    "debug:server": "NODE_ENV=development nodemon -w src/server -w src/common --exec babel-node-debug -c src/server/app.jsx",
    "debug": "npm run debug:server && npm run debug:client",
    "build:server": "BABEL_ENV=production babel src -d lib --ignore client",
    "build:client": "BABEL_ENV=production webpack --config webpack.prod.config.js --colors --progress",
    "build": "npm run build:server && npm run build:client"
  },
  "author": "C. <4019980@qq.com> (https://github.com/xpcode)",
  "license": "ISC",
  "dependencies": {
    "es6-promise": "^3.2.1",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^1.2.0",
    "koa-compress": "^1.0.9",
    "koa-logger": "^1.3.0",
    "koa-react-view": "^2.0.0",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "koa-views": "^4.1.0",
    "lodash": "^4.13.1",
    "react": "^15.2.0",
    "react-dom": "^15.2.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-node5": "^1.2.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.9.2",
    "css-loader": "^0.23.1",
    "file-loader": "^0.9.0",
    "koa-webpack-dev-middleware": "^1.2.1",
    "koa-webpack-hot-middleware": "^1.0.3",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1"
  },
