{
  "name": "ucloud_ui",
  "version": "0.1.0",
  "description": "isomorphic web app for ucloud ui.",
  "repository": "git@github.com:xpcode/react-isomorphic.git",
  "main": "bin/server/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "NODE_ENV=production node bin/server/index.js",
    "install": "",
    "debug:client": "webpack-dev-server --progress --colors --display-error-details --display-modules --config webpack.dev.config.js",
    "debug:server": "NODE_ENV=development nodemon -w src/server -w src/common --exec babel-node --debug src/server/app.jsx",
    "build:server": "BABEL_ENV=production; echo '开始编译：后端程序'; babel src -d bin --ignore client; echo '结束编译：后端程序'",
    "build:client": "BABEL_ENV=production; echo '开始编译：前端程序'; webpack --config webpack.prod.config.js --colors --progress; echo '结束编译：前端程序'",
    "build": "npm run build:server; npm run build:client"
  },
  "keywords": [
    "isomorphic",
    "koa",
    "react",
    "react-router",
    "redux",
    "immutable",
    "nodemon",
    "webpack",
    "babel",
    "es6"
  ],
  "author": "C. <4019980@qq.com> (https://github.com/xpcode)",
  "license": "ISC",
  "dependencies": {
    "es6-promise": "^3.2.1",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "js-beautify": "^1.6.3",
    "koa": "^2.0.0",
    "koa-router": "^7.0.1",
    "koa-static": "^3.0.0",
    "lodash": "^4.13.1",
    "react": "^15.2.0",
    "react-dom": "^15.2.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.9.2",
    "css-loader": "^0.23.1",
    "file-loader": "^0.9.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "nodemon": "^1.9.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.12.1"
  },
  "peerDependencies": {
    "babel-cli": ">=6.10.1"
  },
  "engines": {
    "node": ">=6.2.2",
    "npm": ">=3.10.3"
  }
}
